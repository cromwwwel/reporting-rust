{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Report",
  "type": "object",
  "required": [
    "data",
    "metadata"
  ],
  "properties": {
    "data": true,
    "metadata": {
      "$ref": "#/definitions/Metadata"
    }
  },
  "definitions": {
    "Metadata": {
      "type": "object",
      "required": [
        "created_at",
        "id",
        "title",
        "version"
      ],
      "properties": {
        "author": {
          "type": [
            "string",
            "null"
          ]
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      }
    }
  }
}